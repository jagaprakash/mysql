@using TestingSql.Models
@model IEnumerable<City>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>Country Name</th>
            <th>CountryCode</th>
            <th>District</th>
        </tr>
        @foreach (City customer in Model)
        {
        <tr>
            <td>@customer.Name</td>
            <td>@customer.CountryCode</td>
            <td>@customer.District</td>
        </tr>
        }
    </table>
</body>
</html>